<?xml version="1.0" encoding="utf-8"?>
<ScrollView   xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:background="@color/global_bk"
    android:layout_height="match_parent">

    <HorizontalScrollView android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:tools="http://schemas.android.com/tools"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="@android:color/white">
            <include layout="@layout/header" />
            <LinearLayout
                android:orientation="vertical"
                android:layout_marginTop="80dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:weightSum="1"
                android:layout_gravity="center">
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50"
                    android:layout_marginTop="10dp">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="上车准备开始语音"
                        android:textColor="#ff0000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtPrepareDrivingVoice" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="上车准备时间"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView9"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtAroundCarTimeout"
                        android:layout_weight="0.02" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="结束标志"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02" />

                    <RadioGroup
                        android:layout_width="200dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="车门"
                            android:textColor="@color/black"
                            android:id="@+id/radDoor"
                            android:layout_weight="0.02" />

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="安全带"
                            android:textColor="@color/black"
                            android:id="@+id/radSafeblet"
                            android:layout_weight="0.02" />
                    </RadioGroup>

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">


                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绕车一周"
                        android:id="@+id/chkAroundCarEnable"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="上车准备启用"
                        android:id="@+id/chkPrepareDrivingEnable"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="绕车语音"
                        android:id="@+id/chkAroundCarVoice"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="三模语音"
                        android:id="@+id/chkPrepareDriving3TouchVoice"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="车头检测"
                        android:id="@+id/chkPrepareDrivingHeadStockEnable"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="车尾检测"
                        android:id="@+id/chkPrepareDrivingTailStockEnable"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />



                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="灯光模拟语音"
                        android:textColor="#ff0000"
                        android:textSize="15.0dip" />

                    <EditText
                        android:layout_width="220dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtLightVoice"
                        android:layout_weight="0" />



                    <TextView
                        android:layout_width="130dp"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="灯光模拟检测时间"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtSimulationLightTimeout" />


                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="语音间隔"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtSimulationLightInterval"
                        android:layout_weight="0.12" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="白天"
                        android:id="@+id/chkSimulationsLightOnDay"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="夜间"
                        android:id="@+id/chkSimulationsLightOnNight"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />


                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="起步项目开始语音"
                        android:textColor="#ff0000"
                        android:textSize="15.0dip"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtStartVoice" />



                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="起步项目结束语音"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtStartEndVoice" />


                    <TextView

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="项目距离"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartStopCheckForwardDistance" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="起步限制时间"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartTimeout" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="喇叭检测"
                        android:id="@+id/chkStartLoudSpeakerCheck"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />


                </LinearLayout>

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">


                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="左转向检测"
                        android:id="@+id/chkIsCheckStartLight"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="夜考远近光交替"
                        android:id="@+id/chkStartLowAndHighBeamInNight"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />


                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="起步闯动"
                        android:id="@+id/chkStartShockEnable"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="手刹释放限制"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView10"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="53dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartReleaseHandbrakeTimeout" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="起步转速限制"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView13"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="58dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartEngineRpm" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="闯动加速度"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView14"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="46dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartShockValue" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="闯动次数"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView15"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="69dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStartShockCount" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="靠边停车开始语音"
                        android:textColor="#ff0000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtPullOverVoice" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="靠边停车项目距离"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView9"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtPullOverMaxDrivingDistance"
                        android:layout_weight="0.02" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="停车手刹释放时间"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView16"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="62dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtPullOverHandbrakeTimeout" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="下车时间"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView34"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="62dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtPullOverDoorTimeout" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">


                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="转项灯检测"
                        android:id="@+id/chkPullOverTurnLightCheck"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="开警示灯"
                        android:id="@+id/chkPullOverOpenCautionBeforeGetOff"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="松安全带"
                        android:id="@+id/chkPullOverOpenSafetyBeltBeforeGetOff"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="空挡检测"
                        android:id="@+id/chkPullOverNeutralCheck"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="关发动机"
                        android:id="@+id/chkPullOverStopEngineBeforeGetOff"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="关近光"
                        android:id="@+id/chkPullOverCloseLowBeamBeforeGetOff"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <CheckBox
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="考试自动结束"
                        android:id="@+id/chkPulloverEndAutoTriggerStopExam"
                        android:textColor="@color/black"
                        android:layout_weight="0.02" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="停车标志"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02" />

                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="手刹"
                        android:textColor="@color/black"
                        android:id="@+id/radStopFlagHandBreak"
                        android:layout_weight="0.02" />

                    <RadioButton
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="车停"
                        android:textColor="@color/black"
                        android:id="@+id/radStopFlagCarStop"
                        android:layout_weight="0.02" />

                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="结束标志"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02"
                        android:layout_marginTop="12dp" />

                    <RadioGroup
                        android:layout_width="800dp"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal"
                        android:layout_marginTop="10dp">
                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="开警示灯"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverCautionLight"
                            android:layout_weight="0.02" />

                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="关近光灯"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverLowBeamCheck"
                            android:layout_weight="0.02" />
                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="发动机熄火"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverEngineExtinction"
                            android:layout_weight="0.02" />
                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="松安全带"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverSafetyBelt"
                            android:layout_weight="0.02" />
                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="开关车门"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverOpenCloseDoor"
                            android:layout_weight="0.02" />
                        <RadioButton
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="拉紧驻车制动"
                            android:textColor="@color/black"
                            android:id="@+id/radPullOverHandbrake"
                            android:layout_weight="0.02" />
                    </RadioGroup>
                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="直线行驶开始语音"
                        android:textColor="#ff0000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtStraightDrivingVoice" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="直线行驶结束语音"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView9"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="150dp"
                        android:layout_height="wrap_content"
                        android:inputType="text"
                        android:ems="10"
                        android:id="@+id/edtTxtStraightDrivingEndVoice"
                        android:layout_weight="0.02" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="项目距离"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView16"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:inputType="number"
                        android:ems="10"
                        android:id="@+id/edtTxtStraightDrivingDistance" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="角度检测"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView17"
                        android:layout_weight="0.02" />

                    <EditText
                        android:layout_width="80dp"
                        android:layout_height="wrap_content"
                        android:ems="10"
                        android:id="@+id/edtTxtStraightDrivingMaxOffsetAngle"
                        android:layout_weight="0.02"
                        android:inputType="numberDecimal" />



                </LinearLayout>
                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:layout_weight="0.50">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10.0dip"
                        android:singleLine="true"
                        android:text="直线行驶最低速度"
                        android:textColor="#ff000000"
                        android:textSize="15.0dip"
                        android:id="@+id/textView4"
