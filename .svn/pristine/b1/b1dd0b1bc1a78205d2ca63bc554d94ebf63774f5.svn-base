<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient android:startColor="#ffe9a53c" android:endColor="#ffe98e01" android:angle="90.0" />
            <stroke android:width="0.0dip" android:color="#ff000000" />
            <corners android:radius="3.0dip" />
            <padding android:left="6.0dip" android:top="1.0dip" android:right="6.0dip" android:bottom="1.0dip" />
        </shape>
    </item>
    <item android:state_pressed="false">
        <shape
          xmlns:android="http://schemas.android.com/apk/res/android">
            <gradient android:startColor="#ffe98e01" android:endColor="#ffe9a53c" android:angle="90.0" />
            <stroke android:width="0.0dip" android:color="#ff000000" />
            <corners android:radius="3.0dip" />
            <padding android:left="6.0dip" android:top="1.0dip" android:right="6.0dip" android:bottom="1.0dip" />
        </shape>
    </item>
</selector>