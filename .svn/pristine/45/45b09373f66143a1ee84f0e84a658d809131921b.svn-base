<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/global_bk"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <include
      layout="@layout/header" />
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="考试合格语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView4"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:text="考试合格"
        android:id="@+id/edtTxtExamSuccess"
        android:layout_marginLeft="20dp" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="考试不合格语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView4"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:text="考试不合格,请回考试中心打印成绩"
        android:id="@+id/edtTxtExamFailed" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="开始考试语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView4"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:text="开始考试"
        android:id="@+id/edtTxtStartExamVoice" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="考试结束语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView4"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:text="考试结束"
        android:id="@+id/edtTxtEndExamVoice" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="刹车语音"
        android:id="@+id/chkBreakVoice"
        android:textColor="@color/black"
        android:layout_weight="0.02" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="最大速度保持时间(毫秒)"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="3"
        android:id="@+id/edtTxtCommonExamItemMaxSpeedKeepTime" />

    <CheckBox
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:text="制动不平顺"
       android:id="@+id/chkBrakeIrregularity"
       android:textColor="@color/black"
       android:layout_weight="0.02" />
  </LinearLayout>
  <LinearLayout
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="信号(个)"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="3"
        android:id="@+id/edtTxtBrakeIrregularitySignal" />
    <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginLeft="10.0dip"
           android:singleLine="true"
           android:text="闯动速度"
           android:textColor="#ff000000"
           android:textSize="15.0dip"
           android:id="@+id/textView16"
           android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="3"
        android:id="@+id/edtTxtBrakeIrregularitySpeed" />
    <TextView
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_marginLeft="10.0dip"
           android:singleLine="true"
           android:text="停车信号(个)"
           android:textColor="#ff000000"
           android:textSize="15.0dip"
           android:id="@+id/textView16"
           android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="3"
        android:id="@+id/edtTxtBrakeIrregularitySpeedZero" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="超速信号(个)"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="3"
        android:id="@+id/edtTxtBrakeIrregularitySpeedOver" />
 
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="灯光模拟前是否必须启动发动机"
        android:id="@+id/chkBeforeSimionLightStartEngine"
        android:textColor="@color/black"
        android:layout_weight="0.02" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="启动发动机语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/edtTxtBeforeSimionLightStartEngineVoice" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="启动发动机后延时(秒)"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="2"
        android:id="@+id/edtTxtStartEngineDeleyTime" />

    <CheckBox
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:text="优先播放不合格语音"
    android:id="@+id/chkFirstPlayExamFailVoice"
    android:textColor="@color/black"
    android:layout_weight="0.02" />
    <CheckBox
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:text="播放不合格"
   android:id="@+id/chkPlayFail"
   android:textColor="@color/black"
   android:layout_weight="0.02" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启用全程变道"
        android:id="@+id/chkCommonExamItemsCheckChangeLanes"
        android:textColor="@color/black"
        android:layout_weight="0.02" />
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="启用考试之前拉手刹"
        android:id="@+id/chkCommonExamItemsCheckHandBreake"
        android:textColor="@color/black"
        android:layout_weight="0.02" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="全程变道角度"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="2"
        android:id="@+id/edtTxtCommonExamItemsChangeLanesAngle" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="全程变道时间"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />
    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="2"
        android:id="@+id/edtTxtCommonExamItemsChangeLanesTimeOut" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="减速项目进入后踩刹车才有效（踩着刹车不松开进入项目无效）"
        android:id="@+id/chkBrakeMustInitem"
        android:textColor="@color/black"
        android:layout_weight="0.02" />
  </LinearLayout>
</LinearLayout>