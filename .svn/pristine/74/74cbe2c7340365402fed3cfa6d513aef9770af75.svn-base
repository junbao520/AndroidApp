<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@color/global_bk"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <include layout="@layout/header" />
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="会车项目开始语音"
        android:textColor="#ff0000"
        android:textSize="15.0dip"
        android:id="@+id/textView4"
        android:layout_weight="0.02" />

    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/edtTxtMeetingVoice" />

  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="会车项目结束语音"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView9"
        android:layout_weight="0.02" />

    <EditText
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:inputType="text"
        android:ems="10"
        android:id="@+id/edtTxtMeetingEndVoice"
        android:layout_weight="0.02" />
  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="项目距离"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView16"
        android:layout_weight="0.02" />

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/edtTxtMeetingDrivingDistance" />
    <TextView
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:layout_marginLeft="10.0dip"
         android:singleLine="true"
         android:text="准备距离"
         android:textColor="#ff000000"
         android:textSize="15.0dip"
         android:id="@+id/textView16"
         android:layout_weight="0.02" />
    <EditText
         android:layout_width="80dp"
         android:layout_height="wrap_content"
         android:inputType="number"
         android:ems="10"
         android:id="@+id/edtTxtMeetingPrepareD" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="速度限制"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView17"
        android:layout_weight="0.02" />

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/edtTxtMeetingSlowSpeedInKmh"
        android:layout_weight="0.02" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10.0dip"
        android:singleLine="true"
        android:text="让车角度"
        android:textColor="#ff000000"
        android:textSize="15.0dip"
        android:id="@+id/textView18"
        android:layout_weight="0.02" />

    <EditText
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:inputType="numberDecimal"
        android:ems="10"
        android:id="@+id/edtTxtMeetingAngle"
        android:layout_weight="0.02" />

  </LinearLayout>
  <LinearLayout
      android:layout_width="wrap_content"
      android:layout_height="wrap_content">


    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="必踩刹车"
        android:id="@+id/chkMeetingCheckBrake"
        android:textColor="@color/black"
        android:layout_weight="0.02" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="禁止远光"
        android:id="@+id/chkMeetingForbidHighBeamCheck "
        android:textColor="@color/black"
        android:layout_weight="0.02" />


  </LinearLayout>
</LinearLayout>